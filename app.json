{
  "expo": {
    "name": "Jawwed",
    "slug": "jawwed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Jawwed-Icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/for-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/for-splash.png",
        "backgroundColor": "#000000"
      },
      "package": "com.karim308.jawwed", 
      "usesCleartextTraffic": true,
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "54d22dcb-376a-43a8-b210-2b32bf442b27"
      }
    }
  }
}
