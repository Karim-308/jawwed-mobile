{
  "expo": {
    "name": "Jawwed",
    "slug": "jawwed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Jawwed-Icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/for-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIViewSemanticContentAttribute": "forceRightToLeft"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/for-splash.png",
        "backgroundColor": "#000000"
      },
      "package": "com.karim308.jawwed",
      "usesCleartextTraffic": true,
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "66e3979b-8bd4-4222-9f98-da5e1d5b6f66"
      }
    }
  }
}
